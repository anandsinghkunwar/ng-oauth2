{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [true, "single"],
        "no-namespace": false,
        "no-reference": false,
        "trailing-comma": false,
        "no-string-literal": false
    },
    "jsRules": {
        "quotemark": [true, "single"],
        "trailing-comma": false
    },
    "rulesDirectory": []
}
